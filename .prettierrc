{
  "semi": true,
  "singleQuote": true,
  "printWidth": 80,
  "tabWidth": 4,
  "trailingComma": "es5",
  "useTabs": true,
  "arrowParens": "always",
  "jsxBracketSameLine": false,
  "bracketSameLine": false,
  "bracketSpacing": true,
  "singleAttributePerLine": true,
  "react/jsx-max-props-per-line": [
    1,
    {
      "when": "multiline"
    }
  ]
}